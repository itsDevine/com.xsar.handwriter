<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".NewCustomCamera">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/customCam2Toolbar"
        android:layout_width="match_parent"
        android:layout_height="57dp"
        android:background="@color/colorPrimary"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:layout_anchorGravity="bottom|center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />



    <FrameLayout
        android:id="@+id/customCamFrame"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/customCam2Toolbar"
        app:layout_constraintBottom_toTopOf="@id/customCam2Toolbar2"
        />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/customCam2Toolbar2"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:background="@drawable/customcamlt1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" />

    <Button
        android:id="@+id/customimages21"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:background="@drawable/customcamimages"
        android:visibility="invisible"
        app:layout_constraintTop_toTopOf="@id/customCam2Toolbar2"
        app:layout_constraintBottom_toBottomOf="@id/customCam2Toolbar2"
        app:layout_constraintLeft_toLeftOf="@id/customCam2Toolbar2"
        app:layout_constraintRight_toLeftOf="@id/customflash2"
        />

    <ImageButton
        android:id="@+id/customimages22"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:background="@drawable/customcamimages"
        android:visibility="invisible"
        app:layout_constraintTop_toTopOf="@id/customimages21"
        app:layout_constraintBottom_toBottomOf="@id/customimages21"
        app:layout_constraintLeft_toLeftOf="@id/customimages21"
        app:layout_constraintRight_toRightOf="@id/customimages21"
        />

    <Button
        android:id="@+id/customimagesno2"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:background="@drawable/customcamnumber"
        android:text="4"
        android:textColor="@color/white"
        android:textSize="10dp"
        android:visibility="invisible"
        app:layout_constraintRight_toRightOf="@id/customimages21"
        app:layout_constraintTop_toTopOf="@id/customimages21"
        app:layout_constraintLeft_toRightOf="@id/customimages21"
        app:layout_constraintBottom_toTopOf="@id/customimages21"
        app:layout_constraintVertical_bias="0.49" />

    <Button
        android:id="@+id/customflash2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@color/transparent"
        android:foreground="?android:selectableItemBackground"
        app:layout_constraintTop_toTopOf="@id/customCam2Toolbar2"
        app:layout_constraintBottom_toBottomOf="@id/customCam2Toolbar2"
        app:layout_constraintLeft_toRightOf="@id/customimages21"
        app:layout_constraintRight_toLeftOf="@id/customCamCapture2"
        />

    <ImageView
        android:id="@+id/customflashImage1"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/customcamflashoff"
        android:foreground="?android:selectableItemBackground"
        app:layout_constraintTop_toTopOf="@id/customCam2Toolbar2"
        app:layout_constraintBottom_toBottomOf="@id/customCam2Toolbar2"
        app:layout_constraintLeft_toRightOf="@id/customimages21"
        app:layout_constraintRight_toLeftOf="@id/customCamCapture2"
        />

    <ImageView
        android:id="@+id/customflashImage2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/customcamflashon"
        android:foreground="?android:selectableItemBackground"
        app:layout_constraintTop_toTopOf="@id/customCam2Toolbar2"
        app:layout_constraintBottom_toBottomOf="@id/customCam2Toolbar2"
        app:layout_constraintLeft_toRightOf="@id/customimages21"
        app:layout_constraintRight_toLeftOf="@id/customCamCapture2"
        android:visibility="invisible"
        />

    <ImageView
        android:id="@+id/customflashImage3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/customcamflashauto"
        android:foreground="?android:selectableItemBackground"
        app:layout_constraintTop_toTopOf="@id/customCam2Toolbar2"
        app:layout_constraintBottom_toBottomOf="@id/customCam2Toolbar2"
        app:layout_constraintLeft_toRightOf="@id/customimages21"
        app:layout_constraintRight_toLeftOf="@id/customCamCapture2"
        android:visibility="invisible"
        />

    <Button
        android:id="@+id/customCamCapture2"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/customcapture5"
        android:layout_marginBottom="50dp"
        app:layout_constraintBottom_toBottomOf="@id/customCam2Toolbar2"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

    <Button
        android:id="@+id/customgallery2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/customcamgallery"
        android:visibility="visible"
        app:layout_constraintTop_toTopOf="@id/customCam2Toolbar2"
        app:layout_constraintBottom_toBottomOf="@id/customCam2Toolbar2"
        app:layout_constraintLeft_toLeftOf="@id/customCamCapture2"
        app:layout_constraintRight_toRightOf="@id/customCam2Toolbar2"
        />

    <Button
        android:id="@+id/customnext2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/customtick"
        android:visibility="invisible"
        android:foreground="?android:selectableItemBackground"
        app:layout_constraintBottom_toBottomOf="@id/customCam2Toolbar2"
        app:layout_constraintLeft_toLeftOf="@id/customCamCapture2"
        app:layout_constraintRight_toRightOf="@id/customCam2Toolbar2"
        app:layout_constraintTop_toTopOf="@id/customCam2Toolbar2" />

</androidx.constraintlayout.widget.ConstraintLayout>